<script type="">
	$(window).on('load',function(){
	var url = window.location.href;  
if (url == 'http://www.rupinis.com/') {
	 $(".show").fadeIn();
}

 if (url == 'http://rupinis.com/') {
	 $(".show").fadeIn();
}

$("span, .overlay").click(function(){
$(".show").fadeOut();});});
</script>

